{
  "name": "awaitter-lite",
  "version": "1.0.38",
  "description": "Awaitter Lite - Multi-model AI coding assistant CLI supporting local LLMs, GPT-4, Claude, Gemini, and more",
  "main": "dist/index.js",
  "bin": {
    "awaitter-lite": "./bin/code-cli.js",
    "aw": "./bin/code-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test:smoke": "node dist/index.js --version && echo Test passed: version command works",
    "test:local": "echo TESTING LOCALLY - Run commands manually in test directory",
    "prepublish:check": "node ../scripts/pre-publish-check.js",
    "prepublishOnly": "node ../scripts/prepublish-warning.js && npm run build"
  },
  "keywords": [
    "ai",
    "coding",
    "assistant",
    "cli",
    "llm",
    "gpt",
    "claude",
    "gemini",
    "groq",
    "local",
    "awaitter",
    "ollama",
    "anthropic",
    "openai",
    "code-assistant",
    "source-available",
    "typescript"
  ],
  "author": "NovoLabs",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/awaitter/awaitter-lite.git"
  },
  "homepage": "https://awaitter.com",
  "bugs": {
    "email": "support@awaitter.com",
    "url": "https://github.com/awaitter/awaitter-lite/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/novolabs"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.6.7",
    "chalk": "^4.1.2",
    "cli-highlight": "^2.1.11",
    "commander": "^12.0.0",
    "conf": "^10.2.0",
    "dotenv": "^16.4.4",
    "fast-glob": "^3.3.2",
    "glob": "^10.3.10",
    "inquirer": "^8.2.6",
    "marked": "^12.0.0",
    "marked-terminal": "^7.0.0",
    "minimatch": "^9.0.3",
    "openai": "^6.3.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
